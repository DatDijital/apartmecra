{
  "version": 2,
  "builds": [
    {
      "src": "sekreterlik-app/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd sekreterlik-app/client && npm install && npm run build",
  "outputDirectory": "sekreterlik-app/client/dist"
}
